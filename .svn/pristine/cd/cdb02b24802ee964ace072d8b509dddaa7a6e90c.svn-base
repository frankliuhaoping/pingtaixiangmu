					</div>
                 <!-- 警情性质全称 -->
					<div class="form-group">
						<label for="jqxzqc" class="col-sm-3 control-label"> <spring:message code="pubJqxz.jqxzqc" />: </label>
						<div class="col-sm-9">
							<input id="jqxzqc" name="jqxzqc" class="form-control" readonly="readonly" placeholder="<spring:message code="pubJqxz.jqxzqc" />">
						</div>
					</div>
                   <!-- 标准警情性质编号 -->
					<div class="form-group">
						<label for="bzjqxzbh" class="col-sm-3 control-label"> <spring:message code="pubJqxz.bzjqxzbh" />: </label>
						<div class="col-sm-9">
							<input id="bzjqxzbh" name="bzjqxzbh" class="form-control" readonly="readonly" placeholder="<spring:message code="pubJqxz.bzjqxzbh" />">
						</div>
					</div>
                     <!-- 标准警情性质名称 -->
					<div class="form-group">
						<label for="bzjqxzmc" class="col-sm-3 control-label"> <spring:message code="pubJqxz.bzjqxzmc" />: </label>
						<div class="col-sm-9">
							<input id="bzjqxzmc" name="bzjqxzmc" class="form-control" readonly="readonly" placeholder="<spring:message code="pubJqxz.bzjqxzmc" />">
						</div>
					</div>
                   <!--110用户使用标志  -->
					<div class="form-group">
						<label for="yhsybz110" class="col-sm-3 control-label"> <spring:message code="pubJqxz.yhsybz110" />: </label>
						<div class="col-sm-9">
							<input id="yhsybz110" name="yhsybz110" class="form-control" readonly="readonly" placeholder="<spring:message code="pubJqxz.yhsybz110" />">
						</div>
					</div>
                  <!--122用户使用标志  -->
					<div class="form-group">
						<label for="yhsybz122" class="col-sm-3 control-label"> <spring:message code="pubJqxz.yhsybz122" />: </label>
						<div class="col-sm-9">
							<input id="yhsybz122" name="yhsybz122" class="form-control" readonly="readonly" placeholder="<spring:message code="pubJqxz.yhsybz122" />">
						</div>
					</div>
                  <!-- 120用户使用标志 -->
					<div class="form-group">
						<label for="yhsybz120" class="col-sm-3 control-label"> <spring:message code="pubJqxz.yhsybz120" />: </label>
						<div class="col-sm-9">
							<input id="yhsybz120" name="yhsybz120" class="form-control" readonly="readonly" placeholder="<spring:message code="pubJqxz.yhsybz120" />">
						</div>
					</div>
                  <!--119用户使用标志  -->
					<div class="form-group">
						<label for="yhsybz119" class="col-sm-3 control-label"> <spring:message code="pubJqxz.yhsybz119" />: </label>
						<div class="col-sm-9">
							<input id="yhsybz119" name="yhsybz119" class="form-control" readonly="readonly" placeholder="<spring:message code="pubJqxz.yhsybz119" />">
						</div>
					</div>
                    <!-- 其他用户使用标志 -->
					<div class="form-group">
						<label for="qtyhsybz" class="col-sm-3 control-label"> <spring:message code="pubJqxz.qtyhsybz" />: </label>
						<div class="col-sm-9">
							<input id="qtyhsybz" name="qtyhsybz" class="form-control" readonly="readonly">
						</div>
					</div>
                   <!-- 警情性质预案地址 -->
					<div class="form-group">
						<label for="jqxzyaurl" class="col-sm-3 control-label"> <spring:message code="pubJqxz.jqxzyaurl" />: </label>
						<div class="col-sm-9">
							<input id="jqxzyaurl" name="jqxzyaurl" class="form-control" readonly="readonly">

                  <!-- 数据版本 -->
					<div class="form-group">
						<label for="sjbb" class="col-sm-3 control-label"> <spring:message code="pubJqxz.sjbb" />: </label>
						<div class="col-sm-9">
							<input id="sjbb" name="sjbb" class="form-control" readonly="readonly">
						</div>
					</div>

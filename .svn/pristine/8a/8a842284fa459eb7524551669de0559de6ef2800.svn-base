<!-- js库 -->
	<jsp:include page="/common/template/script_basic.jsp" />
	<jsp:include page="/common/template/script_validate.jsp" />
	<script type="text/javascript" src="<c:url value='/common/static/plugins/switch/js/bootstrap-switch.min.js' />"></script>
	<script type="text/javascript" src="<c:url value='/common/static/js/moment.min.js' />"></script>
	<script type="text/javascript">
		$(function() {
			/* 返回按钮 */
			$('#backBtn').click(function() {
				window.location.href = "<c:url value='/pubRyxx/index' />";
			});
			/* 获取人员信息 参数*/
			var pubRyxx = ${pubRyxx};
			ManageForm.loadForm($('form'), pubRyxx);
			/* 使用标志事件 */
			var sybztate = 'Y' == pubRyxx.sybz ? true : false;
			$("#sybz_checkbox").bootstrapSwitch({
				state : sybztate,
				onText : '<spring:message code="enable" />',
				offText : '<spring:message code="disable" />',
				onColor : 'success',
				offColor : 'danger',
				onSwitchChange : function(event, state) {
					$('#sybz').val(state ? 'Y' : 'N');
				}
			});
			/* 出生年月日期转换 */
			var csny = pubRyxx.csny;//获取出生年月值
			if (csny != null && csny != '') {
				$('#csny').val(moment(csny).format('YYYY-MM-DD HH:mm:ss'));//日期格式
			}
			/* 创建时间日期转换 */
			var cjsj = pubRyxx.cjsj;//获取创建时间值
			if (cjsj != null && cjsj != '') {
				$('#cjsj').val(moment(cjsj).format('YYYY-MM-DD HH:mm:ss'));//日期格式
			}
			/*更新时间日期转换  */
			var gxsj = pubRyxx.gxsj;//获取更新时间值
			if (gxsj != null && gxsj != '') {
				$('#gxsj').val(moment(gxsj).format('YYYY-MM-DD HH:mm:ss'));//日期格式
			}
		});
	</script>
